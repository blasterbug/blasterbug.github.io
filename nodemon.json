{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "src"
  ],
  "verbose": true,
  "exec": "webpack serve",
  "watch": [
    ".env",
    "babel.config.js",
    "postcss.config.js",
    "tailwind.config.js",
    "webpack.config.js"
  ],
  "env": {
    "NODE_ENV": "development",
    "BABEL_ENV": "development"
  },
  "ext": "js,json"
}
