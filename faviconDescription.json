{
  "masterPicture": "./src/logo.svg",
  "iconsPath": "/",
  "design": {
    "ios": {
      "pictureAspect": "backgroundAndMargin",
      "backgroundColor": "#ffffff",
      "margin": "14%",
      "assets": {
        "ios6AndPriorIcons": false,
        "ios7AndLaterIcons": false,
        "precomposedIcons": true,
        "declareOnlyDefaultIcon": true
      },
      "appName": "blasterbug.github.io"
    },
    "desktopBrowser": {
      "design": "background",
      "backgroundColor": "#fff",
      "backgroundRadius": 0.45,
      "imageScale": 0.8
    },
    "windows": {
      "pictureAspect": "whiteSilhouette",
      "backgroundColor": "#0038ff",
      "onConflict": "override",
      "assets": {
        "windows80Ie10Tile": false,
        "windows10Ie11EdgeTiles": {
          "small": false,
          "medium": true,
          "big": false,
          "rectangle": false
        }
      },
      "appName": "blasterbug.github.io"
    },
    "androidChrome": {
      "pictureAspect": "backgroundAndMargin",
      "margin": "17%",
      "backgroundColor": "#ffffff",
      "themeColor": "#ffffff",
      "manifest": {
        "name": "blasterbug.github.io",
        "display": "browser",
        "orientation": "notSet",
        "onConflict": "override",
        "declared": true
      },
      "assets": {
        "legacyIcon": false,
        "lowResolutionIcons": false
      }
    },
    "safariPinnedTab": {
      "pictureAspect": "blackAndWhite",
      "threshold": 92.5,
      "themeColor": "#0038ff"
    }
  },
  "settings": {
    "compression": 5,
    "scalingAlgorithm": "Spline",
    "errorOnImageTooSmall": false,
    "readmeFile": false,
    "htmlCodeFile": true,
    "usePathAsIs": false
  }
}
